    "emailjs-com": "^3.2.0",
    "expo": "~47.0.12",
    "expo-auth-session": "~3.8.0",
    "expo-build-properties": "~0.4.1",
    "expo-constants": "^14.2.1",
    "expo-dev-client": "^2.0.1",
    "expo-device": "^5.2.1",
